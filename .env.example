# Server Configuration
APP_PORT="# Port where the app runs (e.g., 3000, 8080)"

# Database Configuration
DB_HOST="# Database host (e.g., 127.0.0.1, db)"
DB_PORT="# Database port (e.g., 3306 for MySQL)"
DB_USER="# Database username"
DB_PASSWORD="# Database password (keep it empty if no password)"
DB_NAME="# Database name"

# JWT Configuration
JWT_SECRET="# Secret key used to sign JWT tokens"

# CORS Configuration
CORS_ALLOWED_ORIGINS="# Comma-separated list of allowed origins for CORS (no spaces between commas)"
